{
  "name": "cachu",
  "version": "5.4.4",
  "description": "A simple key-value cache for Node.js, Deno, and browsers.",
  "main": "./dist/index.js",
  "browser": "./dist/browser.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./browser": "./dist/browser.js"
  },
  "scripts": {
    "build:node": "esbuild src/node.ts --bundle --minify --format=esm --legal-comments=none --outfile=dist/index.js",
    "build:browser": "esbuild src/deno.ts --bundle --minify --format=esm --legal-comments=none --outfile=dist/browser.js",
    "build": "rimraf dist && npm run build:node && npm run build:browser",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azurydev/cachu.git"
  },
  "keywords": [
    "kv",
    "cache",
    "production-ready",
    "asynchronous",
    "modules",
    "simple"
  ],
  "author": "Samuel Kopp <opensource@azury.dev> (https://azury.dev)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/azurydev/cachu/issues"
  },
  "homepage": "https://github.com/azurydev/cachu#readme",
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.6",
    "@changesets/cli": "^2.24.1",
    "@types/jest": "^28.1.6",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "esbuild": "^0.14.49",
    "eslint": "^8.20.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^28.1.3",
    "ms": "^2.1.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "type": "module"
}
