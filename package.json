{
  "name": "cachu",
  "version": "3.6.0",
  "description": "ü¶ù Simple, Minimalistic KV Cache",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/node.js",
      "types": "./types/node.d.ts"
    },
    "./browser": {
      "import": "./build/browser.js",
      "types": "./types/browser.d.ts"
    }
  },
  "scripts": {
    "dev": "tsc -w",
    "build": "packu --node --esm -i src/node.ts -o build/node.js",
    "build:browser": "packu --node --esm -i src/browser.ts -o build/browser.js",
    "publish:patch": "tsc && npm run build && npm run build:browser && npm version patch && npm publish",
    "publish:minor": "tsc && npm run build && npm run build:browser && npm version minor && npm publish",
    "publish:major": "tsc && npm run build && npm run build:browser && npm version major && npm publish",
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest"
  },
  "funding": {
    "type": "individual",
    "url": "https://ko-fi.com/azury"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azurydev/cachu.git"
  },
  "keywords": [
    "kv",
    "cache",
    "easy-to-use",
    "minimalistic",
    "async",
    "modular"
  ],
  "author": "Azury <hello@azury.dev> (https://azury.dev)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/azurydev/cachu/issues"
  },
  "homepage": "https://github.com/azurydev/cachu#readme",
  "devDependencies": {
    "jest": "^27.4.7",
    "packu": "^2.1.0",
    "redis": "^4.0.2",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "type": "module"
}
