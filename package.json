{
  "name": "cachu",
  "version": "6.0.0-canary.4",
  "description": "A modern and simple key-value cache for Node.js.",
  "types": "types/index.d.ts",
  "exports": "./dist/index.js",
  "scripts": {
    "build": "rimraf dist && rimraf types && tsc && esbuild src/index.ts --bundle --minify --format=esm --legal-comments=none --platform=node --outfile=dist/index.js",
    "version:canary": "npm version prerelease --preid=canary",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azurydev/cachu.git"
  },
  "keywords": [
    "kv",
    "cache",
    "production-ready",
    "asynchronous",
    "modules",
    "simple"
  ],
  "author": "Samuel Kopp <opensource@azury.dev> (https://azury.dev)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/azurydev/cachu/issues"
  },
  "homepage": "https://github.com/azurydev/cachu#readme",
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.5",
    "@changesets/cli": "^2.23.1",
    "@types/jest": "^28.1.6",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.29.0",
    "esbuild": "^0.14.49",
    "eslint": "^8.18.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^28.1.3",
    "ms": "^2.1.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "type": "module"
}
